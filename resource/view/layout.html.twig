{% extends "base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ app.request.basepath }}/css/vendor.min.css" />
    <link rel="stylesheet" type="text/css" href="{{ app.request.basepath }}/css/app.min.css" />
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ app.request.basepath }}/js/vendor.min.js"></script>
    <script src="{{ app.request.basepath }}/js/app.min.js"></script>
{% endblock %}

{% block body %}
    {{ parent() }}
    {% block content %}
    {% endblock %}
{% endblock %}
